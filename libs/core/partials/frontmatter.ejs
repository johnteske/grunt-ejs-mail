<% if(!production){ %>
<div id="frontmatter-wrapper">
    <div id="frontmatter">
        From: <%= data.from %><br />
        Subject: <%= data.subject %><br />
        Preview: <%= data.preview %><br />
        utm_source: <%= data.utm_source %><br />
        Last updated: <%= Date().toString().split("GMT")[0]; %><br />
    </div>
</div>
<% } %>
